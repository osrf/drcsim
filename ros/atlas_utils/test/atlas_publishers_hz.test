<launch>
  <!-- Bring up gazebo without the GUI -->
  <include file="$(find atlas_utils)/launch/atlas.launch">
    <arg name="gzname" value="gzserver"/>
  </include>

  <!-- Test for nominal publication rates -->
  <test pkg="rostest" type="hztest" test-name="atlas_laser_hztest">
    <param name="hz" value="40.0"/>
    <param name="hzerror" value="20.0"/>
    <param name="topic" value="scan"/>
    <param name="test_duration" value="10.0"/>
  </test>
  <test pkg="rostest" type="hztest" test-name="atlas_joint_states_hztest">
    <param name="hz" value="100.0"/>
    <param name="hzerror" value="20.0"/>
