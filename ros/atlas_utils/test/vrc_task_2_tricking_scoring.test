<launch>
  <env name="VRC_CHEATS_ENABLED" value="1"/>
  <include file="$(find atlas_utils)/launch/vrc_task_2.launch">
    <arg name="gzname" value="gzserver"/>
  </include>
  <test pkg="atlas_utils" type="vrc_task_2_tricking_scoring_test" 
        test-name="vrc_task_2_tricking_scoring" 
        time-limit="360.0">
       <param name="pose_gate1" type="str" value="2.81 -38.07 1.00" />
       <param name="gates_to_pass" value="0 1 2 3 4"
       <param name="expected_score" value="0 1 2 3 4"
       <param name="expected_elapsed" value="0 1 1 1 1"
  </test>
</launch>
