<launch>

  <arg name="gzname" default="gazebo"/>

  <!-- start gazebo with the DRC Robot -->
  <param name="/use_sim_time" type="bool" value="true"/>

  <!-- start gazebo with the DRC Robot -->
  <node name="gazebo" pkg="drc_robot_utils" type="run_$(arg gzname)" args="drc_sim_v0.world"/>

  <include file="$(find drc_robot_utils)/launch/drc_robot_bringup.launch"/>
</launch>
  

