<launch>
  <env name="VRC_CHEATS_ENABLED" value="2"/>
  <include file="$(find drcsim_gazebo)/launch/vrc_task_2.launch">
    <arg name="gzname" value="gzserver"/>
  </include>
  <include file="$(find drcsim_gazebo)/test/pub_atlas_command.launch" />

  <param name="/gzstop_checker/time_to_publish" value="15" />
  <param name="/gzstop_checker/logfile"         value="/tmp/vrc_task_2/state.log" /> 
  <param name="/gzstop_checker/results_postfix" value="vrc_task_2" /> 
  <test pkg="drcsim_gazebo" type="gzlog_stop_checker.py" 
        test-name="vrc_task_2_gzlog_stop" 
        time-limit="360" />
</launch>
